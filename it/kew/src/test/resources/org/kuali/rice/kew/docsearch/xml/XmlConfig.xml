		<!-- Test the XStreamSafeEvaluator against searchable attributes to resolve EN-63 and KULWF-723 -->
		<ruleAttribute>
			<name>XMLXStreamSearchableAttribute</name>
			<className>org.kuali.rice.kew.docsearch.xml.StandardGenericXMLSearchableAttribute</className>
			<label>XML XStream Searchable attribute</label>
			<description>XML XStream Searchable attribute</description>
			<type>SearchableXmlAttribute</type>
			<searchingConfig>
				<fieldDef name="givenname" title="First name">
					<display>
						<type>text</type>
					</display>
					<validation required="true">
						<regex>^[a-zA-Z ]+$</regex>
						<message>Invalid first name</message>
					</validation>
					<fieldEvaluation>
						<xpathexpression>wf:xstreamsafe('//putWhateverWordsIwantInsideThisTag/givenname/value')</xpathexpression>
					</fieldEvaluation>
				</fieldDef>
				<xmlSearchContent>
					<putWhateverWordsIwantInsideThisTag>
						<givenname>
							<value reference="../../thisIsTheRealSlimShadyValue"/>
						</givenname>
						<thisIsTheRealSlimShadyValue>%givenname%</thisIsTheRealSlimShadyValue>
					</putWhateverWordsIwantInsideThisTag>
				</xmlSearchContent>
			</searchingConfig>
		</ruleAttribute>
