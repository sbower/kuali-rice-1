		<!-- Test the use of a quickfinder to resolve EN-95 -->
		<ruleAttribute>
			<name>SearchableAttributeWithQuickfinder</name>
			<className>org.kuali.rice.kew.docsearch.xml.StandardGenericXMLSearchableAttribute</className>
			<label>Searchable attribute wth Quickfinder</label>
			<description>Searchable attribute wth Quickfinder</description>
			<type>SearchableXmlAttribute</type>
			<searchingConfig>
				<fieldDef name="chart" title="Chart">
					<display>
						<type>text</type>
					</display>
					<!-- the businessObjectClass and lookupFieldName are completey fake here because we aren't actually going to render this thing -->
					<lookup businessObjectClass="org.kuali.rice.kew.docsearch.xml.MyLookupable">
					  <fieldConversions>
					    <fieldConversion lookupFieldName="chart" localFieldName="MyBean.data"/>
					  </fieldConversions>
					</lookup>
					<fieldEvaluation>
						<xpathexpression>//chart</xpathexpression>
					</fieldEvaluation>
				</fieldDef>
				<xmlSearchContent>
					<chart>%chart%</chart>
				</xmlSearchContent>
			</searchingConfig>
		</ruleAttribute>
		<!-- Test the hiding of columns and the use of hidden fields -->
		<ruleAttribute>
			<name>SearchableAttributeWithHiddens</name>
			<className>org.kuali.rice.kew.docsearch.xml.StandardGenericXMLSearchableAttribute</className>
			<label>Searchable attribute wth Hidden fields and columns</label>
			<description>Searchable attribute wth Hidden fields and columns</description>
			<type>SearchableXmlAttribute</type>
			<searchingConfig>
				<fieldDef name="chart" title="Chart">
					<display>
						<type>text</type>
					</display>
					<visibility>
						<column visible="false"/>
					</visibility>
					<lookup businessObjectClass="org.kuali.rice.shareddata.impl.campus.CampusBo">
					  <fieldConversions>
					    <fieldConversion lookupFieldName="chart" localFieldName="chart" />
					  </fieldConversions>
					</lookup>
					<fieldEvaluation>
						<xpathexpression>//chart</xpathexpression>
					</fieldEvaluation>
				</fieldDef>
				<fieldDef name="org" title="Org">
					<display>
						<type>hidden</type>
					</display>
					<fieldEvaluation>
						<xpathexpression>//org</xpathexpression>
					</fieldEvaluation>
				</fieldDef>
				<fieldDef name="dollar" title="Dollar">
					<display>
						<type>text</type>
					</display>
					<visibility>
						<field visible="false"/>
					</visibility>
					<fieldEvaluation>
						<xpathexpression>//dollar</xpathexpression>
					</fieldEvaluation>
				</fieldDef>
				<xmlSearchContent>
					<chartOrg><chart>%chart%</chart><org>%org%</org><dollar>%dollar%</dollar></chartOrg>
				</xmlSearchContent>
			</searchingConfig>
		</ruleAttribute>
