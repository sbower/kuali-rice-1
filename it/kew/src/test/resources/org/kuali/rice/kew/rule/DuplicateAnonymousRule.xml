<?xml version="1.0" encoding="UTF-8"?>
<!-- Rule used in RuleXmlParserTest.testAnonymousDuplicatesNamed test.  This anonymous rule will duplicate the named rule, "ANamedRule" -->
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
    <rules xmlns="ns:workflow/Rule" xsi:schemaLocation="ns:workflow/Rule resource:Rule">
        <rule>
            <!-- duplicate of: <name>ANamedRule</name>  -->
            <documentType>TestDocumentType</documentType>
            <ruleTemplate>TestRuleTemplate</ruleTemplate>
            <description>Duplicate of a named rule</description>
            <forceAction>false</forceAction>
            <ruleExtensions>
                <ruleExtension>
                    <attribute>TestRuleAttribute</attribute>
                    <ruleTemplate>TestRuleTemplate</ruleTemplate>
                    <ruleExtensionValues>
                        <ruleExtensionValue>
                            <key>color</key>
                            <value>green</value>
                        </ruleExtensionValue>
                        <ruleExtensionValue>
                            <key>shape</key>
                            <value>square</value>
                        </ruleExtensionValue>
                    </ruleExtensionValues>
                </ruleExtension>
            </ruleExtensions>
            <responsibilities>
                <responsibility>
                    <user>user1</user>
                    <actionRequested>A</actionRequested>
                    <priority>1</priority>
                </responsibility>
            </responsibilities>
        </rule>
    </rules>
</data>
